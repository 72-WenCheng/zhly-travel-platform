{
  "dashboard": {
    "id": null,
    "title": "旅游系统监控面板",
    "tags": ["travel", "spring-boot"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "系统概览",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"travel-server\"}",
            "legendFormat": "应用状态"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "请求响应时间",
        "type": "graph",
        "targets": [
          {
            "expr": "http_server_requests_seconds{job=\"travel-server\"}",
            "legendFormat": "响应时间"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "JVM内存使用",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"travel-server\"}",
            "legendFormat": "已使用内存"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"travel-server\"}",
            "legendFormat": "最大内存"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "数据库连接池",
        "type": "graph",
        "targets": [
          {
            "expr": "hikaricp_connections_active{job=\"travel-server\"}",
            "legendFormat": "活跃连接"
          },
          {
            "expr": "hikaricp_connections_idle{job=\"travel-server\"}",
            "legendFormat": "空闲连接"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Redis连接",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_connected_clients{job=\"redis\"}",
            "legendFormat": "连接数"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}


















